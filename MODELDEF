Model UnchargedBusterTracer // Name of actor
{
   Path "Models/Projectiles" // Path to model in PK3
   SKIN 0 "tracer1.png" // Model index, model file
   MODEL 0 "Tracer.md3" // Model index, texture (can be in any format supported by GZDoom)
   Scale 4.5 5 5 // Scale values L/W/H
   USEACTORPITCH // A flag that will fix the model pitch for projectiles

   FrameIndex BNTR A 0 0 // The sprite lump, sprite frame, model index, frame number
}

